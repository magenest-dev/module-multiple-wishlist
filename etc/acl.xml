<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
               <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_MultipleWishlist::multiplewishlist_config" title="Multiple Wishlist" />
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Reports::report">
                    <resource id="Magenest_MultipleWishlist::Report_MultipleWishlist" title="Report MultipleWishlist">
                        <resource id="Magenest_MultipleWishlist::Report_Wishlist" title="Report Wishlist" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>